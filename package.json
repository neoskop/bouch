{
  "name": "@neoskop/bouch",
  "version": "0.3.1",
  "description": "CouchDB/PouchDB Backup/Restore Tool with Attachments Support",
  "keywords": [
    "couchdb",
    "pouchdb",
    "backup",
    "restore",
    "migration",
    "database",
    "cli",
    "typescript"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Mark Wecke <wecke@neoskop.de>",
  "license": "MIT",
  "private": false,
  "bin": {
    "bouch": "./bin/bouch.js"
  },
  "scripts": {
    "build": "tsc",
    "publish-next": "npm run build && npm publish --tag next",
    "publish-latest-only": "npm run build && npm publish",
    "publish-latest": "npm run publish-latest-only && npm dist-tag add @neoskop/bouch@`jq '.version' package.json -r` next"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/bson": "4.0.0",
    "@types/dateformat": "3.0.0",
    "@types/fs-extra": "5.0.5",
    "@types/minimatch": "3.0.3",
    "@types/node": "11.12.2",
    "@types/progress": "2.0.3",
    "@types/sort-object-keys": "1.1.0",
    "@types/yargs": "12.0.10",
    "typescript": "3.4.1"
  },
  "dependencies": {
    "axios": "0.18.1",
    "bson": "4.0.2",
    "dateformat": "3.0.3",
    "fs-extra": "7.0.1",
    "minimatch": "3.0.4",
    "progress": "2.0.3",
    "rxjs": "6.4.0",
    "sort-object-keys": "1.1.2",
    "tslib": "1.9.3",
    "yargs": "13.2.2"
  }
}